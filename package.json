{
  "name": "webhook",
  "version": "0.0.1",
  "description": "webhook resta pi",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec babel-node src/server.js --presets es2015,stage-0",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist/server.js",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "author": "Ferran Julia <ferran.julia@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "boom": "^7.2.0",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "helmet": "^3.13.0",
    "morgan": "^1.9.0",
    "serve-favicon": "^2.5.0",
    "winston": "^3.0.0"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^5.3.0",
    "nodemon": "^1.18.3"
  }
}
